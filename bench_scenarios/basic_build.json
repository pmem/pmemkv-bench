{
    "db_bench": {
        "repo_url": "https://github.com/pmem/pmemkv-bench.git",
        "commit": "HEAD",
        "env": {}
    },
    "pmemkv": {
        "repo_url": "https://github.com/pmem/pmemkv.git",
        "commit": "HEAD",
        "cmake_params": [
            "-DCMAKE_BUILD_TYPE=Release",
            "-DENGINE_CMAP=1",
            "-DENGINE_CSMAP=1",
            "-DENGINE_RADIX=1",
            "-DENGINE_STREE=1",
            "-DBUILD_JSON_CONFIG=1",
