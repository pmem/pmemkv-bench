        "env": {
            "CC": "gcc",
            "CXX": "g++"
        }
    },
    "libpmemobjcpp": {
        "repo_url": "https://github.com/pmem/libpmemobj-cpp.git",
        "commit": "HEAD",
        "cmake_params": [
            "-DBUILD_EXAMPLES=OFF",
            "-DBUILD_TESTS=OFF",
            "-DBUILD_DOC=OFF",
            "-DBUILD_BENCHMARKS=OFF",
            "-DCMAKE_BUILD_TYPE=Release"
        ],
        "env": {
            "CC": "gcc",
            "CXX": "g++"
        }
    }
